{
  "dashboard": {
    "title": "Archon Agent Performance Dashboard",
    "tags": ["archon", "agents", "aal", "performance"],
    "timezone": "browser",
    "schemaVersion": 38,
    "version": 1,
    "refresh": "30s",
    "panels": [
      {
        "id": 1,
        "type": "stat",
        "title": "Total AAL Requests (24h)",
        "gridPos": { "x": 0, "y": 0, "w": 6, "h": 4 },
        "targets": [
          {
            "expr": "sum(increase(aal_requests_total[24h]))",
            "legendFormat": "Total Requests",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "thresholds": {
              "mode": "absolute",
              "steps": [
                { "value": null, "color": "green" },
                { "value": 1000, "color": "yellow" },
                { "value": 5000, "color": "red" }
              ]
            },
            "unit": "short"
          }
        }
      },
      {
        "id": 2,
        "type": "stat",
        "title": "Total Cost (24h)",
        "gridPos": { "x": 6, "y": 0, "w": 6, "h": 4 },
        "targets": [
          {
            "expr": "sum(increase(aal_cumulative_cost_usd[24h]))",
            "legendFormat": "Cost USD",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "thresholds": {
              "mode": "absolute",
              "steps": [
                { "value": null, "color": "green" },
                { "value": 50, "color": "yellow" },
                { "value": 100, "color": "red" }
              ]
            },
            "unit": "currencyUSD",
            "decimals": 2
          }
        }
      },
      {
        "id": 3,
        "type": "stat",
        "title": "Success Rate (24h)",
        "gridPos": { "x": 12, "y": 0, "w": 6, "h": 4 },
        "targets": [
          {
            "expr": "sum(increase(aal_requests_total{status=\"success\"}[24h])) / sum(increase(aal_requests_total[24h])) * 100",
            "legendFormat": "Success %",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "thresholds": {
              "mode": "absolute",
              "steps": [
                { "value": null, "color": "red" },
                { "value": 90, "color": "yellow" },
                { "value": 95, "color": "green" }
              ]
            },
            "unit": "percent",
            "decimals": 1
          }
        }
      },
      {
        "id": 4,
        "type": "stat",
        "title": "Avg Response Time",
        "gridPos": { "x": 18, "y": 0, "w": 6, "h": 4 },
        "targets": [
          {
            "expr": "avg(rate(aal_requests_latency_seconds_sum[5m]) / rate(aal_requests_latency_seconds_count[5m]))",
            "legendFormat": "Avg Latency",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "thresholds": {
              "mode": "absolute",
              "steps": [
                { "value": null, "color": "green" },
                { "value": 5, "color": "yellow" },
                { "value": 10, "color": "red" }
              ]
            },
            "unit": "s",
            "decimals": 2
          }
        }
      },
      {
        "id": 5,
        "type": "bargauge",
        "title": "Cost per Provider (Last 30 Days)",
        "gridPos": { "x": 0, "y": 4, "w": 12, "h": 8 },
        "targets": [
          {
            "expr": "sum by (provider) (increase(aal_cumulative_cost_usd[30d]))",
            "legendFormat": "{{provider}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "thresholds": {
              "mode": "absolute",
              "steps": [
                { "value": null, "color": "green" },
                { "value": 100, "color": "yellow" },
                { "value": 500, "color": "red" }
              ]
            },
            "unit": "currencyUSD"
          }
        },
        "options": {
          "orientation": "horizontal",
          "displayMode": "gradient"
        }
      },
      {
        "id": 6,
        "type": "timeseries",
        "title": "Request Rate by Provider",
        "gridPos": { "x": 12, "y": 4, "w": 12, "h": 8 },
        "targets": [
          {
            "expr": "sum by (provider) (rate(aal_requests_total[5m]))",
            "legendFormat": "{{provider}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "smooth",
              "fillOpacity": 10
            },
            "unit": "reqps"
          }
        }
      },
      {
        "id": 7,
        "type": "timeseries",
        "title": "Token Efficiency by Provider",
        "gridPos": { "x": 0, "y": 12, "w": 12, "h": 8 },
        "targets": [
          {
            "expr": "sum by (provider) (rate(aal_tokens_total{token_type=\"output\"}[5m])) / sum by (provider) (rate(aal_tokens_total{token_type=\"input\"}[5m]))",
            "legendFormat": "{{provider}} (output/input ratio)",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "smooth"
            },
            "unit": "short",
            "decimals": 2
          }
        }
      },
      {
        "id": 8,
        "type": "gauge",
        "title": "Circuit Breaker States",
        "gridPos": { "x": 12, "y": 12, "w": 12, "h": 8 },
        "targets": [
          {
            "expr": "aal_circuit_breaker_state",
            "legendFormat": "{{provider}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "thresholds": {
              "mode": "absolute",
              "steps": [
                { "value": 0, "color": "green" },
                { "value": 1, "color": "yellow" },
                { "value": 2, "color": "red" }
              ]
            },
            "mappings": [
              { "type": "value", "value": 0, "text": "CLOSED" },
              { "type": "value", "value": 1, "text": "HALF_OPEN" },
              { "type": "value", "value": 2, "text": "OPEN" }
            ],
            "unit": "short"
          }
        },
        "options": {
          "showThresholdMarkers": true,
          "showThresholdLabels": true
        }
      },
      {
        "id": 9,
        "type": "table",
        "title": "Provider Cost Breakdown (30d)",
        "gridPos": { "x": 0, "y": 20, "w": 12, "h": 8 },
        "targets": [
          {
            "expr": "sum by (provider, model) (increase(aal_cumulative_cost_usd[30d]))",
            "legendFormat": "{{provider}} - {{model}}",
            "refId": "A",
            "format": "table",
            "instant": true
          }
        ],
        "transformations": [
          {
            "id": "organize",
            "options": {
              "renameByName": {
                "provider": "Provider",
                "model": "Model",
                "Value": "Cost (USD)"
              }
            }
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "currencyUSD",
            "decimals": 2
          },
          "overrides": [
            {
              "matcher": { "id": "byName", "options": "Provider" },
              "properties": [
                { "id": "custom.width", "value": 150 }
              ]
            }
          ]
        }
      },
      {
        "id": 10,
        "type": "heatmap",
        "title": "Request Latency Distribution",
        "gridPos": { "x": 12, "y": 20, "w": 12, "h": 8 },
        "targets": [
          {
            "expr": "sum(rate(aal_requests_latency_seconds_bucket[5m])) by (le)",
            "legendFormat": "{{le}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "custom": {
              "hideFrom": {
                "tooltip": false,
                "viz": false,
                "legend": false
              }
            }
          }
        }
      },
      {
        "id": 11,
        "type": "timeseries",
        "title": "Provider Fallback Events",
        "gridPos": { "x": 0, "y": 28, "w": 12, "h": 8 },
        "targets": [
          {
            "expr": "sum by (from_provider, to_provider) (rate(aal_routing_fallbacks_total[5m]))",
            "legendFormat": "{{from_provider}} â†’ {{to_provider}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "custom": {
              "drawStyle": "bars"
            },
            "unit": "ops"
          }
        }
      },
      {
        "id": 12,
        "type": "timeseries",
        "title": "Cost Savings via Circuit Breaker",
        "gridPos": { "x": 12, "y": 28, "w": 12, "h": 8 },
        "targets": [
          {
            "expr": "sum(aal_circuit_breaker_state{state=\"open\"}) * 0.1",
            "legendFormat": "Estimated Savings (USD/h)",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "smooth",
              "fillOpacity": 20
            },
            "unit": "currencyUSD"
          }
        }
      }
    ]
  }
}
